{
  "manifest_version": 3,
  "name": "AEM QuickLinks Extension",
  "version": "1.0",
  "description": "Toolbar extension with useful AEM links.",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png",
      "32": "logo32.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "permissions": ["tabs"],
  "commands": {
    "open-extension": {
      "suggested_key": {
        "default": "Ctrl+Y"
      },
      "description": "Open AEM QuickLinks Extension popup"
    }
  }
}
